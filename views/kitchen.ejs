                              </div>
                              <!-- <span class="icon is-left">
                                    <i class="sl sl-icon-magnifier"></i>
                                 </span> -->
                           </h3>
                           <table class="table hire-table order_table table-fixed" id="table-id">
                              <thead>
                                 <th>Table No</th>
                                 <th>Token No</th>
                                 <th>Food Name</th>
                                 <th>Food Type</th>
                                 <th>Food Quantity</th>
                                 <th>Comments</th>
                                 <th>Order Status</th>
                                 <th>Food Order Status</th>
                              </thead>
                              <tbody class="order_table_body">
                                 <% for(var i=0; i < data[0].result.length; i++) { %>
                                 <tr class="<%= data[0].result[i].food_order_id %> cloneRow">
                                    <td>
                                       <h4><span class="label label-success"><span
                                                class="table_No"><%= data[0].result[i].table_id %></span><i
                                                class="fa fa-cutlery" aria-hidden="true"></i></span></h4>
                                    </td>
                                    <td>
                                       <h4><span class="label label-success"><span
                                                class="token_No:"><%= data[0].result[i].token_no %></span><i
                                                class="fa fa-cutlery" aria-hidden="true"></i></span></h4>
                                    </td>
                                    <td>
                                       <h4><span class="label label-success"><span
                                                class="food_Name"><%= data[0].result[i].food_name %></span>
                                          </span></h4>
                                    </td>
                                    <td>
                                       <h4><span class="label label-success"><span class="food_Category">
                                                <%if (data[0].result[i].is_veg == 1) { %>
                                                VEG
                                                <% }else{ %>
                                                NON-VEG
                                                <% } %>
                                             </span></span>
                                       </h4>
                                    </td>
                                    <td>
                                       <h4><span class="label label-success"><span
                                                class="food_Count"><%= data[0].result[i].food_count %></span></span>
                                       </h4>
                                    </td>
                                    <td>
                                       <h4><span class="label label-success"><span
                                                class="comments"><%= data[0].result[i].food_comments %></span><i
                                                class="fa fa-comments-o" aria-hidden="true"></i></span></h4>
                                    </td>
                                    <td>
                                       <h4><span class="label label-success"><span
                                                class="order_Status_main order_Status-<%= data[0].result[i].order_id %>"><%= data[0].result[i].order_status_main %></span><i
                                                class="fa fa-comments-o" aria-hidden="true"></i></span></h4>
                                    </td>
                                    <td>
                                       <select class='food-select order_Status'
                                          data-value='<%= data[0].result[i].food_order_id %>'
                                          id="food-select-option-<%= data[0].result[i].food_order_id %>">
                                          <option class='opt-food'>Select Option</option>
                                          <option class='opt-food' value='PLACED'>
                                             <P>Placed</P>
                                          </option>
                                          <option class='opt-food' value='COMPLETED'>Completed</option>
                                          <option class='opt-food' value='SERVED'>Served</option>
                                          <option class='opt-food' value='REJECTED'>Rejected</option>
                                          <option class='opt-food' value='CONFIRMED'>Confirmed</option>
                                       </select>
                                       <script>
                                          $('#food-select-option-<%= data[0].result[i].food_order_id %>').val('<%= data[0].result[i].order_status %>');
                                       </script>
                                       <% if(data[0].result[i].order_status == 'REJECTED') { %>
                                       <script>
                                          $('#food-select-option-<%= data[0].result[i].food_order_id %>').addClass('order_Status_rejected');
                                       </script>
                                       <% }else{ %>
                                       <script>
                                          $('#food-select-option-<%= data[0].result[i].food_order_id %>').removeClass('order_Status_rejected');
                                       </script>
                                       <% } %>
                                    </td>
                                 </tr>
                                 <% } %>
                              </tbody>
